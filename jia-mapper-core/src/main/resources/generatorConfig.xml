<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>

	<!-- 配置mysql 驱动jar包路径.用了绝对路径 -->
	<classPathEntry location="D:\workspace\project\mysql-connector-java-5.1.26.jar" />

	<context id="jia-mapper-core_mysql_tables" targetRuntime="MyBatis3">

		<!-- 为了防止生成的代码中有很多注释，比较难看，加入下面的配置控制 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true" />
			<property name="suppressDate" value="true" />
		</commentGenerator>
		<!-- 注释控制完毕 -->

		<!-- 数据库连接 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://wydiy.com:3306/12349_e2tt_com?characterEncoding=utf8"
			userId="isp" password="mymail321">
		</jdbcConnection>

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<!-- 数据表对应的model 层 -->
		<javaModelGenerator targetPackage="cn.jia.core.entity" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<!-- sql mapper 隐射配置文件 -->
		<sqlMapGenerator targetPackage="mapper.cn.jia.core" targetProject="src/main/resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>

		<!-- 在ibatis2 中是dao层，但在mybatis3中，其实就是mapper接口 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="cn.jia.core.dao" targetProject="src/main/java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- <table schema="jia" tableName="core_dict" domainObjectName="Dict" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
		</table>
		<table schema="jia" tableName="core_log" domainObjectName="Log" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
			<columnOverride column="param" javaType="java.lang.String" jdbcType="VARCHAR" />
		</table>
		-->
		<table schema="jia" tableName="core_action" domainObjectName="Action" enableCountByExample="false"
			enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
		</table>
		<!--
		<table schema="jia" tableName="core_file" domainObjectName="File" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
		</table>

		<table schema="jia" tableName="core_notice" domainObjectName="Notice" enableCountByExample="false"
			   enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false" selectByExampleQueryId="false">
			<generatedKey column="id" sqlStatement="Mysql" identity="true" />
		</table>
		 -->
	</context>
</generatorConfiguration>