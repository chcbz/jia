<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.jia.core.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.jia.core.entity.DictEntity">
        <id column="ID" property="id" />
        <result column="UPDATE_TIME" property="updateTime" />
        <result column="CREATE_TIME" property="createTime" />
        <result column="TYPE" property="type" />
        <result column="LANGUAGE" property="language" />
        <result column="NAME" property="name" />
        <result column="VALUE" property="value" />
        <result column="URL" property="url" />
        <result column="PARENT_ID" property="parentId" />
        <result column="DICT_ORDER" property="dictOrder" />
        <result column="DESCRIPTION" property="description" />
        <result column="STATUS" property="status" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        UPDATE_TIME,
        CREATE_TIME,
        ID, TYPE, LANGUAGE, NAME, VALUE, URL, PARENT_ID, DICT_ORDER, DESCRIPTION, STATUS
    </sql>

</mapper>
